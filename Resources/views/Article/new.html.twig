{% extends "InstinctNewsBundle::layout.html.twig" %}


{% block instinct_news_content %}
<h1>Article creation</h1>

<form action="{{ path('instinct_news_admin_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('instinct_news_home') }}">
            Back to the list
        </a>
    </li>
</ul>

{% endblock %}